package chapter4;

public class Student {
	String name;
	int age;
	String fullName;
	String firstName;
	String emailAddress;

	public Student() {
		System.out.println("Learn Java with me");
	}

	public Student(String name, int age) {
		this.name = name;
		this.age = age;
	}

	String getAge() {
		return "DinhLoc";
	}
}
